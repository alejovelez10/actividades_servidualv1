<p id="notice"><%= notice %></p>

<h1>Rols</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Act admin</th>
      <th>Act basico</th>
      <th>Act director</th>
      <th>Doc admin</th>
      <th>Doc basico</th>
      <th>Doc</th>
      <th>Director</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rols.each do |rol| %>
      <tr>
        <td><%= rol.name %></td>
        <td><%= rol.description %></td>
        <td><%= rol.act_admin %></td>
        <td><%= rol.act_basico %></td>
        <td><%= rol.act_director %></td>
        <td><%= rol.doc_admin %></td>
        <td><%= rol.doc_basico %></td>
        <td><%= rol.doc %></td>
        <td><%= rol.director %></td>
        <td><%= link_to 'Show', rol %></td>
        <td><%= link_to 'Edit', edit_rol_path(rol) %></td>
        <td><%= link_to 'Destroy', rol, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rol', new_rol_path %>
