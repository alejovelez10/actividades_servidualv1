<p id="notice"><%= notice %></p>

<h1>Actividads</h1>

<table>
  <thead>
    <tr>
      <th>Consecutivo</th>
      <th>Tipo</th>
      <th>User</th>
      <th>Prioridad</th>
      <th>Responsable</th>
      <th>Nombre</th>
      <th>Descripcion</th>
      <th>Anexo</th>
      <th>F entrega</th>
      <th>Porcentaje</th>
      <th>Estado envio</th>
      <th>Contador</th>
      <th>Codigo</th>
      <th>E vencimiento</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @actividads.each do |actividad| %>
      <tr>
        <td><%= actividad.consecutivo %></td>
        <td><%= actividad.tipo %></td>
        <td><%= actividad.user_id %></td>
        <td><%= actividad.prioridad %></td>
        <td><%= actividad.responsable_id %></td>
        <td><%= actividad.nombre %></td>
        <td><%= actividad.descripcion %></td>
        <td><%= actividad.anexo %></td>
        <td><%= actividad.f_entrega %></td>
        <td><%= actividad.porcentaje %></td>
        <td><%= actividad.estado_envio %></td>
        <td><%= actividad.contador %></td>
        <td><%= actividad.codigo %></td>
        <td><%= actividad.e_vencimiento %></td>
        <td><%= link_to 'Show', actividad %></td>
        <td><%= link_to 'Edit', edit_actividad_path(actividad) %></td>
        <td><%= link_to 'Destroy', actividad, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Actividad', new_actividad_path %>
